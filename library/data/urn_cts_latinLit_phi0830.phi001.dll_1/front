                <!-- Secondary sources, i.e, articles, commentaries, books, etc. -->

                <tei-div id="bibliography-secondary-sources" data-teiname="div" type="bibliography" xml:id="bibliography-secondary-sources">
                    <tei-head data-teiname="head">Secondary Sources</tei-head>
                    <tei-listbibl id="secondary-sources" data-teiname="listBibl" xml:id="secondary-sources">

                        <tei-bibl id="Baehr1870" data-teiname="bibl" xml:id="Baehr1870"><tei-abbr data-teiname="abbr" type="siglum">Baehr. 1870</tei-abbr> =
