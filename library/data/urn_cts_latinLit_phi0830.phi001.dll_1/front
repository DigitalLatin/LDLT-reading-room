                                                <tei-handnote id="δ1" data-teiname="handNote" xml:id="δ1"><tei-abbr data-teiname="abbr" type="siglum">δ<tei-hi data-teiname="hi" rend="super">1</tei-hi></tei-abbr></tei-handnote>
                                                <tei-handnote id="δ2" data-teiname="handNote" xml:id="δ2"><tei-abbr data-teiname="abbr" type="siglum">δ<tei-hi data-teiname="hi" rend="super">2</tei-hi></tei-abbr></tei-handnote>
