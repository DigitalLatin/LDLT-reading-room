                        <tei-person id="Argol." data-teiname="person" xml:id="Argol.">
                            <tei-persname data-teiname="persName" ref="http://viaf.org/viaf/34563690">
                                <tei-abbr data-teiname="abbr" type="siglum">Argol.</tei-abbr> = <tei-forename data-teiname="forename">Giovanni</tei-forename>
                                <tei-surname data-teiname="surname">Argoli</tei-surname>
                            </tei-persname>
                            <tei-note data-teiname="note">Notes published in <tei-bibl data-teiname="bibl"><tei-author data-teiname="author">Onuphrii Panvinii</tei-author>,
