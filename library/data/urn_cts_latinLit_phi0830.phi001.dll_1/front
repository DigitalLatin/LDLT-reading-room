                                <tei-witness id="γ" data-teiname="witness" xml:id="γ"><tei-abbr data-teiname="abbr" type="siglum">γ</tei-abbr> = <tei-title data-teiname="title">Codex
                                        Vaticanus 3152</tei-title>
                                    <tei-msdesc data-teiname="msDesc">
