                     <tei-rdg data-teiname="rdg" wit="#n #b" source="#i #g #l2 #Gryph. #Tig." ana="#orthographical">acantida</tei-rdg>
                  </tei-app> nuper</tei-l>
               <tei-l data-teiname="l" n="77">diceris in silva iudex audisse <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem" wit="#λ #β #α #ρ #δ #ε #γ #π" source="#edd.">Thalea</tei-lem>
                     <tei-rdg data-teiname="rdg" wit="#N #G #κ #χ" ana="#orthographical">talea</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#θ #ν #η #r #s" source="#Wernsd." ana="#orthographical">thalia</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#φ" ana="#orthographical #lexical">talia</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#μ" ana="#lexical">tegea</tei-rdg>
                     <tei-rdg data-teiname="rdg" source="#Tig." ana="#orthographical" type="conjecture">Thalaea</tei-rdg>
                     <tei-rdg data-teiname="rdg" source="#Tit." ana="#lexical #morphological" type="conjecture">Theloni</tei-rdg>
                     <tei-rdg data-teiname="rdg" source="#Heins." ana="#lexical" type="conjecture">Galesa</tei-rdg>
                  </tei-app>.</tei-l>
            
            
               
