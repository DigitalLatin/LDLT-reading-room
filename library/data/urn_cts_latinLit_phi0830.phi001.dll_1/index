                    <tei-note data-teiname="note" target="poem5#rdg5.120-label">praef. <tei-ref data-teiname="ref" target="front#G">G</tei-ref></tei-note>
            </tei-app>sed iam <tei-app data-teiname="app">
                  <tei-lem data-teiname="lem">sera</tei-lem>
