                        <tei-lg id="C4strophe10" data-teiname="lg" type="strophe" n="10" xml:id="C4strophe10">
                            <tei-l data-teiname="l" n="127"><tei-label data-teiname="label" type="speaker"><tei-app data-teiname="app">
                                <tei-lem data-teiname="lem">A.</tei-lem>
                                <tei-rdg data-empty="data-empty" data-teiname="rdg" wit="#G" ana="#subtractive"></tei-rdg>
                            </tei-app></tei-label>Ille meis pacem dat montibus: ecce per illum,</tei-l>
