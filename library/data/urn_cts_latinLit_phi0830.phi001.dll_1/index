                                        </tei-physdesc>
                                        <tei-history data-teiname="history">
                                            <tei-origin data-teiname="origin">
                                                <tei-p data-teiname="p">Written carelessly in the <tei-origdate data-teiname="origDate" notAfter="1500" notBefore="1400">fifteenth
                                                  century</tei-origdate>.</tei-p>
                                            </tei-origin>
                                        </tei-history>
                                    </tei-msdesc>
                                </tei-witness>
                                <tei-witness id="γ" data-teiname="witness" xml:id="γ"><tei-abbr data-teiname="abbr" type="siglum">γ</tei-abbr> = <tei-title data-teiname="title">Codex
                                        Vaticanus 3152</tei-title>
                                    <tei-msdesc data-teiname="msDesc">
                                        <tei-msidentifier data-teiname="msIdentifier">
                                            <tei-settlement data-teiname="settlement">Vatican City</tei-settlement>
                                            <tei-repository data-teiname="repository">Biblioteca Apostolica Vaticana</tei-repository>
                                            <tei-idno data-teiname="idno">Vaticanus 3152</tei-idno>
                                        </tei-msidentifier>
                                        <tei-mscontents data-teiname="msContents">
                                            <tei-p data-teiname="p">It contains Calpurnius’ eleven eclogues (1–18ʳ),
                                                followed by various poems by Cyprian, Lactantius,
                                                Firmianus, and Ausonius.</tei-p>
                                        </tei-mscontents>
                                        <tei-physdesc data-teiname="physDesc">
                                            <tei-objectdesc data-teiname="objectDesc">
                                                <tei-p data-teiname="p"><tei-material data-teiname="material">Paper</tei-material> : 215 × 147 mm. : 81
                                                  leaves. It consists of 81 leaves, of which 18ᵛ,
                                                  26–30, 51ʳ, 81ᵛ are blank. There are 31 verses on
                                                  each page.</tei-p>
                                            </tei-objectdesc>
                                            <tei-handdesc data-teiname="handDesc" hands="2">
                                                <tei-handnote id="γ1" data-teiname="handNote" xml:id="γ1"><tei-abbr data-teiname="abbr" type="siglum">γ<tei-hi data-teiname="hi" rend="super">1</tei-hi></tei-abbr></tei-handnote>
                                                <tei-handnote id="γ2" data-teiname="handNote" xml:id="γ2"><tei-abbr data-teiname="abbr" type="siglum">γ<tei-hi data-teiname="hi" rend="super">2</tei-hi></tei-abbr></tei-handnote>
