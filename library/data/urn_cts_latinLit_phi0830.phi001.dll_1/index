                  <tei-wit data-teiname="wit">edd. ante <tei-ref data-teiname="ref" target="front#Glaeser">Glaeser</tei-ref></tei-wit>
               </tei-app> fronde caduca</tei-l>
            <tei-l data-teiname="l" n="117"><tei-app data-teiname="app">
                  <tei-lem data-teiname="lem" wit="#V" source="#edd.">sterne</tei-lem>
