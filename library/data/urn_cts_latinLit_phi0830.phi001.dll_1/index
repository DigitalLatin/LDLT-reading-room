                  <tei-rdg data-teiname="rdg" wit="#G" ana="#lexical #subtractive">sint <tei-gap data-empty="data-empty" data-teiname="gap" quantity="2" unit="character" reason="lost"></tei-gap></tei-rdg>
               </tei-app> solum, ne forte rigor <tei-app data-teiname="app">
                  <tei-lem data-teiname="lem">penetrabile</tei-lem>
                  <tei-rdg data-teiname="rdg" wit="#N" ana="#orthographical">penetrabille</tei-rdg>
               </tei-app> corpus</tei-l>
            <tei-l data-teiname="l" n="118">urat et <tei-app data-teiname="app">
