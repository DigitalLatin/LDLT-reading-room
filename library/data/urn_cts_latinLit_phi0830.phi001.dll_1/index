                  <tei-rdg data-teiname="rdg" wit="#χ" ana="#lexical"><tei-add data-teiname="add" place="margin">ramum</tei-add></tei-rdg>
               </tei-app>
               <tei-app data-teiname="app">
                  <tei-lem data-teiname="lem">et constringere</tei-lem>
                  <tei-rdg data-teiname="rdg" source="#Tit. #Heins." ana="#lexical #subtractive" type="conjecture">et stringere</tei-rdg>
               </tei-app> frondes,</tei-l>
            <tei-l data-teiname="l" n="110"><tei-app data-teiname="app">
