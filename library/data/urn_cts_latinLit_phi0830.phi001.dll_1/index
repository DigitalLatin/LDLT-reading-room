                     <tei-rdg data-teiname="rdg" source="#Burm1731" ana="#lexical">lentus</tei-rdg>
                  </tei-app> ades, <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem" wit="#γ #χ #ε #κ #φ #ν" source="#edd.">Lycida</tei-lem>
                     <!-- Last letter is hard to see. -->
                     <tei-rdg id="rdg6.01-licidas" data-teiname="rdg" wit="#N" xml:id="rdg6.01-licidas" ana="#orthographical">licida<tei-del data-teiname="del" rend="erasure">s</tei-del></tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#G #β #π #α #λ #ρ #μ #d2" ana="#orthographical">licida</tei-rdg>
                  </tei-app>: modo <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem" wit="#ν #γ #ρ2" source="#edd.">Nyctilus</tei-lem>
                     <tei-rdg data-teiname="rdg" wit="#N" ana="#orthographical">nictulus</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#G #V #d2" source="#l2" ana="#orthographical">nictilus</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#β" ana="#orthographical">nictilius</tei-rdg>
                  </tei-app> et puer Alcon</tei-l>
               <tei-l data-teiname="l" n="2">certavere sub <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">his</tei-lem>
                     <tei-rdg data-teiname="rdg" wit="#G" ana="#orthographical">hys</tei-rdg>
                  </tei-app> alterno carmine ramis,</tei-l>
               <tei-l data-teiname="l" n="3">iudice me, <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">sed</tei-lem>
                     <tei-rdg data-empty="data-empty" data-teiname="rdg" wit="#G" ana="#subtractive"></tei-rdg>
                  </tei-app>
