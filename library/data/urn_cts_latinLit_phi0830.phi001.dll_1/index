                     <tei-rdg data-teiname="rdg" wit="#π" ana="#lexical">retusas</tei-rdg>
                  </tei-app>)</tei-l>
               <tei-l data-teiname="l" n="29">arbiter <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem" wit="#N #G #H" source="#Heins.">inflatis non</tei-lem>
