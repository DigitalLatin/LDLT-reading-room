                  </tei-app>
                  <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">fuit</tei-lem>
                     <tei-rdg data-teiname="rdg" source="#Heins." ana="#lexical" type="conjecture">ruit</tei-rdg>
                     <tei-rdg id="rdg7.2fugit" data-teiname="rdg" source="#Maehly" xml:id="rdg7.2fugit" ana="#lexical" type="conjecture">fugit</tei-rdg>
                     <tei-note data-teiname="note" target="poem7#rdg7.2fugit">(qui et <tei-hi data-teiname="hi" rend="italic">lux</tei-hi> pro <tei-hi data-teiname="hi" rend="italic">nox</tei-hi> corr.)</tei-note>
                     <tei-rdg data-teiname="rdg" source="#Baehr." ana="#lexical" type="conjecture">subit</tei-rdg>
                  </tei-app>, <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">ut</tei-lem>
