                            <tei-cell data-teiname="cell" role="data"><tei-quote lang="la" data-teiname="quote" xml:lang="la">contentus</tei-quote> or <tei-quote lang="la" data-teiname="quote" xml:lang="la">contemptus</tei-quote></tei-cell>
                        </tei-row>
