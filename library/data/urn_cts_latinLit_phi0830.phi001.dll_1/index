                  <tei-rdg data-teiname="rdg" wit="#V" source="#edd." ana="#morphological">incurvare velis</tei-rdg>
                  <tei-rdg id="rdg5.109incursare_velet" data-teiname="rdg" source="#Haupt1854 #Baehr. #Schenkl1 #Keene" xml:id="rdg5.109incursare_velet" ana="#lexical" type="conjecture">incursare vetet</tei-rdg>
                  <tei-note data-teiname="note" target="poem5#rdg5.109incursare_velet">(<tei-ref data-teiname="ref" target="front#Haupt1854">Haupt</tei-ref> l. l. p. 31)</tei-note>
                  <!-- Haupt (l. l. p. 31) -->
               </tei-app> velit <tei-app data-teiname="app">
                  <tei-lem data-teiname="lem">nemus</tei-lem>
                  <tei-rdg data-teiname="rdg" wit="#ν #φ #η #θ #r" source="#i1 #Tit. #Vienn. #Heins." ana="#lexical">vimen</tei-rdg>
