                     <tei-rdg data-teiname="rdg" wit="#β #e #u #b" ana="#orthographical">vigessima</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#N #φ" ana="#orthographical">vicessima</tei-rdg>
                  </tei-app> certe</tei-l>
               <tei-l data-teiname="l" n="2"><tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">nox</tei-lem>
