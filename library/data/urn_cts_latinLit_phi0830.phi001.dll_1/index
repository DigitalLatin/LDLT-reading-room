                <!-- Beginning of poem 5 -->
                <tei-div id="poem5" data-teiname="div" type="textpart" n="5" xml:id="poem5">
            <tei-head data-teiname="head">Poem 5.</tei-head>
            <tei-l data-teiname="l" n="1">Forte <tei-app data-teiname="app">
                  <tei-lem data-teiname="lem" wit="#G #V" source="#Baehr. #Schenkl">Micon</tei-lem>
                  <tei-rdg data-teiname="rdg" wit="#N" ana="#orthographical">michon</tei-rdg>
                  <tei-rdg data-teiname="rdg" wit="#γ #ν #ε2" source="#edd." ana="#orthographical">mycon</tei-rdg>
               </tei-app> senior <tei-app data-teiname="app">
                  <tei-lem data-teiname="lem" wit="#ν #α #ρ2 #λ2" source="#edd.">Canthusque</tei-lem>
