                  </tei-app> vicibus <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">certare</tei-lem>
                     <tei-rdg data-teiname="rdg" wit="#G" ana="#morphological"><tei-add data-teiname="add" place="margin">certate</tei-add></tei-rdg>
                     <!-- G in mg. -->
                  </tei-app> placet—<tei-app data-teiname="app">
