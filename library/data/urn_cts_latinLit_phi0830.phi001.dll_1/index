            <tei-sourcedesc id="sources" data-teiname="sourceDesc" xml:id="sources">
                <tei-bibl id="Giarratano1910" data-teiname="bibl" xml:id="Giarratano1910" n="Giarratano 1910"><tei-title data-teiname="title" level="m">Calpurnii et
                        Nemesiani Bucolica</tei-title>, <tei-editor data-teiname="editor" ref="http://viaf.org/viaf/79016214"><tei-forename data-teiname="forename">Caesar</tei-forename><tei-surname data-teiname="surname">Giarratano</tei-surname></tei-editor>,
                        <tei-pubplace data-teiname="pubPlace">Naples</tei-pubplace>, <tei-publisher data-teiname="publisher">Detken et Rocholl</tei-publisher>,
