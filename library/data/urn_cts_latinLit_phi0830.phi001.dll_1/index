                     <tei-witdetail data-teiname="witDetail" wit="#N" target="poem6#rdg6.03-non-om" type="correction-original">add. <tei-ref data-teiname="ref" target="front#N2">N<tei-hi data-teiname="hi" rend="superscript">2</tei-hi></tei-ref></tei-witdetail>
                  </tei-app> sine pignore. <tei-app data-teiname="app">
                     <tei-lem data-teiname="lem">Nyctilus</tei-lem>
