               <tei-l data-teiname="l" n="4">iuncta matre <tei-app data-teiname="app">
                     <tei-lem id="lem6.4dedit" data-teiname="lem" xml:id="lem6.4dedit">dedit</tei-lem>
                     <tei-rdg id="rdg6.04-deo" data-teiname="rdg" wit="#N" xml:id="rdg6.04-deo" ana="#lexical">deo <tei-gap data-empty="data-empty" data-teiname="gap" reason="illegible" extent="unknown" unit="character"></tei-gap></tei-rdg>
                     <tei-witdetail data-teiname="witDetail" wit="#N" target="poem6#rdg6.04-deo" type="correction-original">corr. <tei-ref data-teiname="ref" target="front#N2">N<tei-hi data-teiname="hi" rend="superscript">2</tei-hi></tei-ref></tei-witdetail>
                </tei-app>: catulum dedit <tei-app data-teiname="app">
                     <tei-lem id="lem6.5ille" data-teiname="lem" xml:id="lem6.5ille">ille</tei-lem>
                     <tei-note data-teiname="note" target="poem6#lem6.5ille">nonulli, ut <tei-ref data-teiname="ref" target="front#p">Pithoeus</tei-ref>, <tei-ref data-teiname="ref" target="front#Ulit.">Ulit.</tei-ref> etc., post <tei-hi data-teiname="hi" rend="italic">ille</tei-hi>
                        interpunxerunt.</tei-note>
                  </tei-app>
                  <tei-app data-teiname="app">
                     <tei-lem id="lem6.4Lacaenae" data-teiname="lem" source="#Heins. #Mueller" xml:id="lem6.4Lacaenae" type="emendation">Lacaenae</tei-lem>
                     <!-- Lacaenae (vel Laconem Heins.) Heins. L. Mueller (l. l. p. 1072) -->
                     <tei-note data-teiname="note" target="poem6#lem6.4Lacaenae">L. Mueller (l. l. p. 1072)</tei-note>
                     <tei-rdg data-teiname="rdg" source="#Heins." ana="#lexical #morphological">Laconem</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#N #π #μ #δ" ana="#orthographical">leene</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#G #χ" source="#edd." ana="#orthographical">leaenae</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#ε #κ #λ #α #γ #η #φ #β #r #e #u #d2" ana="#orthographical">leenae</tei-rdg>
                     <tei-rdg data-teiname="rdg" wit="#ρ #ν" ana="#orthographical">laeene</tei-rdg>
