                                <tei-rdg data-teiname="rdg" wit="#μ" ana="#orthographical">agustas</tei-rdg>
                            </tei-app> feret haec <tei-app data-teiname="app">
                                <tei-lem data-teiname="lem">Meliboeus</tei-lem>
                                <tei-rdg data-teiname="rdg" wit="#ν #ε2 #ρ2" ana="#orthographical">meliboeus</tei-rdg>
