               </tei-app> vastet <tei-app data-teiname="app">
                  <tei-lem id="lem5.118pecuaria" data-teiname="lem" wit="#N #G #A #π" source="#edd." xml:id="lem5.118pecuaria">pecuaria</tei-lem>
