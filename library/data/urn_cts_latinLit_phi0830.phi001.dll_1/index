                  <tei-lem data-teiname="lem">coactis</tei-lem>
                  <tei-rdg id="rdg5.108coactus" data-teiname="rdg" source="#Glaeser" xml:id="rdg5.108coactus" ana="#morphological" type="conjecture">coactus</tei-rdg>
                  <tei-note data-teiname="note" target="poem5#rdg5.108coactus">(ex cod. Pal. <tei-ref data-teiname="ref" target="front#Barth1613">Barthii</tei-ref>)</tei-note>
                  <!-- Glaeser (ex cod. Pal. Barthii). -->
               </tei-app></tei-l>
            <tei-l data-teiname="l" n="109"><tei-app data-teiname="app">
                  <tei-lem id="lem5.109incurvare_velit" data-teiname="lem" wit="#N #G" source="#Martell. #Ulit. #h #t #Ellis #Schenkl2" xml:id="lem5.109incurvare_velit">incurvare velit</tei-lem>
                  <tei-note data-teiname="note" target="poem5#lem5.109incurvare_velit">(<tei-ref data-teiname="ref" target="front#Ellis">Ellis</tei-ref> l. l. p.
                     31)</tei-note>
