                            </tei-app>
                            <tei-app data-teiname="app">
                                <tei-lem data-teiname="lem">sonum modulemur avena</tei-lem>
                                <tei-rdg data-teiname="rdg" source="#Wernsd." ana="#morphological" type="conjecture">sono modulemur avenae</tei-rdg>
                            </tei-app>:</tei-l>
                        <tei-l data-teiname="l" n="94"><tei-app data-teiname="app">
