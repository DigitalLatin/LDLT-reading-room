                  <tei-rdg data-teiname="rdg" wit="#V" source="#edd." ana="#morphological #lexical">tremuli tremebunda</tei-rdg>
                  <tei-rdg data-teiname="rdg" source="#Maehly" ana="#lexical" type="conjecture">tremulo iam non</tei-rdg>
                  <tei-rdg id="rdg5.65tremuli_dent_mulctra" data-teiname="rdg" source="#Wernsd. #Beck #Keene" xml:id="rdg5.65tremuli_dent_mulctra" ana="#morphological #lexical" type="conjecture">tremuli
                     dent mulctra</tei-rdg>
                  <tei-note data-teiname="note" target="poem5#rdg5.65tremuli_dent_mulctra">(<tei-ref data-teiname="ref" target="front#Beck">Beck</tei-ref>
                     volebat etiam <tei-hi data-teiname="hi" rend="italic">ac denses</tei-hi> vel <tei-hi data-teiname="hi" rend="italic">atque
                        premas tremebunda</tei-hi>)</tei-note>
                  <!-- Beck (qui volebat etiam denses vel atque premas tremebunda) -->
